<?xml version="1.0" encoding="UTF-8"?>
<!-- The above is a default, but its inclusion should be everywhere just to indicate that all files, for our purposes, are UTF-8 encoded.  Also, as a reminder, the less than, greater than, ampersand, apostrophe, and quotation mark are invalid XML and must be replaced with the literal text &lt; &gt; &amp; &apos; &quot; with the semicolons -->
<core_data>
  <spritesheet name="Map Selector">
    <mSheetTypeName>WorldMap</mSheetTypeName>
	<mPath>_Previs Map Objects/World_Map/map selector SS</mPath>
	<mSpriteList>
	  <sprite> <!-- name here does not actually matter, neither does order of sprites -->
	    <id>0</id> <!-- integer, ids must be unique and flush -->
		<name>sprite</name> <!-- simply for debug purposes -->
		<frames>1</frames> <!-- must be an integer, 1 or higher -->
		<y_origin>0</y_origin> <!-- the top of an image is 0 -->
		<width>102</width>
		<height>102</height>
		<fps>0</fps> <!-- must be non-negative; 0 will turn into 1.0d so be sure that there is only one frame if you put 0 -->
	  </sprite>
	</mSpriteList>
    <mTagList></mTagList>
  </spritesheet>
  
